<!DOCTYPE html>
<html lang="<%= lang %>">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="<%= description %>">
    <title><%= title %></title>
</head>

<body>
    <script type="module">
        await import('/local-data.js');
        const type = '<%= type %>';
        const key = '<%= key %>';
        const selector = '<%= selector %>';
        const keys = '<%= keys %>'.split(',');
        if (keys.includes(selector))
            localData.history[type].update(key, selector);
        window.location.href = `/${localData.lang}/${type}/${key}`;
    </script>
</body>

</html>